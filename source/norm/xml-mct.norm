include "XML.Grammar"
redefine empty_elem_tag                           %[44]
    '< [name] [repeat attribute ordered ] [opt spaces]'/>        
end define

redefine stag                                     %[40]
    '< [name] [repeat attribute ordered ] [opt spaces]'>
end define

redefine attribute                                %[41]
    [spaces] [name] '= [attvalue]
end define
