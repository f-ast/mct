package org.eclipse.gmf.internal.xpand.expression.ast;

abstract public class Expression extends SyntaxElement implements Analyzable, Evaluatable {

    protected EClassifier findType (final Identifier type, final ExecutionContext ctx, final Set < AnalysationIssue > issues);
    abstract protected Object evaluateInternal (ExecutionContext ctx);
    public Expression (final int start, final int end, final int line) {
    }

    final public Object evaluate (final ExecutionContext ctx);
}

