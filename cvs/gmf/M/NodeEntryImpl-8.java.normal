package org.eclipse.gmf.runtime.notation.impl;

public class NodeEntryImpl extends NotationEObjectImpl implements BasicEMap.Entry {

    protected EClass eStaticClass (); protected Node key;

    protected NodeEntryImpl () {
    }

    protected int hash = - 1;
    final protected static Alignment VALUE_EDEFAULT = Alignment.CENTER_LITERAL;
    final protected static int VALUE_EFLAG = 0x7 << VALUE_EFLAG_OFFSET;
    final protected static int VALUE_EFLAG_DEFAULT = Alignment.VALUES.indexOf (VALUE_EDEFAULT) << VALUE_EFLAG_OFFSET;
    final protected static int VALUE_EFLAG_OFFSET = 8;

    public Alignment getTypedValue ();
    public EMap getEMap ();
    public Node basicGetTypedKey ();
    public Node getTypedKey ();
    public Object eGet (int featureID, boolean resolve, boolean coreType);
    public Object getKey ();
    public Object getValue ();
    public Object setValue (Object value);
    public String toString ();
    public boolean eIsSet (int featureID);
    public int getHash ();
    public void eSet (int featureID, Object newValue);
    public void eUnset (int featureID);
    public void setHash (int hash);
    public void setKey (Object key);
    public void setTypedKey (Node newKey);
    public void setTypedValue (Alignment newValue);
}

