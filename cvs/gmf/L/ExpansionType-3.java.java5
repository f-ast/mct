package org.eclipse.gmf.runtime.common.ui.dialogs;

import java.util.Arrays;

import java.util.Collections;

import java.util.List;

import org.eclipse.gmf.runtime.common.core.util.EnumeratedType;

public class ExpansionType extends EnumeratedType {
    private static int nextOrdinal = 0;
    public static final ExpansionType NONE = new ExpansionType ("None");
    public static final ExpansionType INCOMING = new ExpansionType ("Incoming");
    public static final ExpansionType OUTGOING = new ExpansionType ("Outgoing");
    public static final ExpansionType BOTH = new ExpansionType ("Both");
    public static final ExpansionType ALL = new ExpansionType ("All");
    public static final ExpansionType [] VALUES = {NONE, INCOMING, OUTGOING, BOTH, ALL};

    protected ExpansionType (String name, int ordinal) {
        super (name, ordinal);
    }

    private ExpansionType (String name) {
        this (name, nextOrdinal ++);
    }

    protected List getValues () {
        return Collections.unmodifiableList (Arrays.asList (VALUES));
    }

}

