import org.eclipse.gmf.runtime.notation.Guide;

class GuideMap {
    private Guide _horizontal;
    private Guide _vertical;

    public Guide getHorizontal () {
        return _horizontal;
    }

    public void setHorizontal (Guide toSet) {
        _horizontal = toSet;
    }

