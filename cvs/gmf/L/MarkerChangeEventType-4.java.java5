package org.eclipse.gmf.runtime.common.ui.internal.resources;

import java.util.Arrays;

import java.util.Collections;

import java.util.List;

import org.eclipse.gmf.runtime.common.core.util.EnumeratedType;

public class MarkerChangeEventType extends EnumeratedType {
    private static final long serialVersionUID = 1L;
    private static int nextOrdinal = 0;
    public static final MarkerChangeEventType ADDED = new MarkerChangeEventType ("Added");
    public static final MarkerChangeEventType REMOVED = new MarkerChangeEventType ("Removed");
    public static final MarkerChangeEventType CHANGED = new MarkerChangeEventType ("Changed");
    private static final MarkerChangeEventType [] VALUES = {ADDED, REMOVED, CHANGED};

    public MarkerChangeEventType (String name, int ordinal) {
        super (name, ordinal);
    }

    private MarkerChangeEventType (String name) {
        this (name, nextOrdinal ++);
    }

    protected List getValues () {
        return Collections.unmodifiableList (Arrays.asList (VALUES));
    }

}

