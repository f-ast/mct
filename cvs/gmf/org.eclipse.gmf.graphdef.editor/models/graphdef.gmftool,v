text
@<?xml version="1.0" encoding="UTF-8"?>
<gmftool:ToolRegistry xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <palette
      title="gmfgraph palette"
      description="gmfgraph palette">
    <tools
        xsi:type="gmftool:ToolGroup"
        title="Diagram Elements"
        description="Can visualize domain model elements"
        collapsible="true">
      <tools
          xsi:type="gmftool:CreationTool"
          title="Node"
          description="Create Diagram Element representing Top-level diagram Node">
