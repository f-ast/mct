public class SharedBehaviourImpl extends EObjectImpl implements SharedBehaviour {
	/**
	 * The cached value of the '{@@link #getDelegate() <em>Delegate</em>}' reference.
	 * <!-- begin-user-doc -->
	 * <!-- end-user-doc -->
	 * @@see #getDelegate()
	 * @@generated
	 * @@ordered
	 */
	protected Behaviour delegate;

	/**
	 * <!-- begin-user-doc -->
	 * <!-- end-user-doc -->
