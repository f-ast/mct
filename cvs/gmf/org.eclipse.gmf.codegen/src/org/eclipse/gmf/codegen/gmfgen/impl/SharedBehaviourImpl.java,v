			case GMFGenPackage.SHARED_BEHAVIOUR__SUBJECT:
				return eBasicSetContainer(null, GMFGenPackage.SHARED_BEHAVIOUR__SUBJECT, msgs);
		}
		return super.eInverseRemove(otherEnd, featureID, msgs);
	}

