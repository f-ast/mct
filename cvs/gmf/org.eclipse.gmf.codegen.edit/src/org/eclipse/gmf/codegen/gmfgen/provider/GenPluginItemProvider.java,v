				 getResourceLocator(),
				 getString("_UI_GenPlugin_iD_feature"),
				 getString("_UI_PropertyDescriptor_description", "_UI_GenPlugin_iD_feature", "_UI_GenPlugin_type"),
				 GMFGenPackage.eINSTANCE.getGenPlugin_ID(),
