 * @@author artem
 */
public interface NamingStrategy {

	String get(CanvasMapping mapping);

	String get(NodeMapping mapping);

	String get(LinkMapping mapping);

	String get(CompartmentMapping mapping);

	String get(LabelMapping mapping);
}
