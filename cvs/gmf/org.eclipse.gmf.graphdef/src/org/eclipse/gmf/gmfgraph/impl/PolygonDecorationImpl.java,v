	 */
	public boolean eIsSet(EStructuralFeature eFeature) {
		switch (eDerivedStructuralFeatureID(eFeature)) {
			case GMFGraphPackage.POLYGON_DECORATION__NAME:
				return NAME_EDEFAULT == null ? name != null : !NAME_EDEFAULT.equals(name);
			case GMFGraphPackage.POLYGON_DECORATION__CHILDREN:
				return children != null && !children.isEmpty();
			case GMFGraphPackage.POLYGON_DECORATION__PARENT:
				return getParent() != null;
			case GMFGraphPackage.POLYGON_DECORATION__COLOR_STYLE:
				return colorStyle != null;
			case GMFGraphPackage.POLYGON_DECORATION__SIZE_STYLE:
				return sizeStyle != null;
			case GMFGraphPackage.POLYGON_DECORATION__FONT_STYLE:
				return fontStyle != null;
			case GMFGraphPackage.POLYGON_DECORATION__LOCATION:
				return location != null;
			case GMFGraphPackage.POLYGON_DECORATION__SIZE:
				return size != null;
			case GMFGraphPackage.POLYGON_DECORATION__LAYOUT_MANAGER:
				return LAYOUT_MANAGER_EDEFAULT == null ? layoutManager != null : !LAYOUT_MANAGER_EDEFAULT.equals(layoutManager);
			case GMFGraphPackage.POLYGON_DECORATION__OUTLINE:
				return outline != OUTLINE_EDEFAULT;
			case GMFGraphPackage.POLYGON_DECORATION__FILL:
				return fill != FILL_EDEFAULT;
			case GMFGraphPackage.POLYGON_DECORATION__LINE_WIDTH:
				return lineWidth != LINE_WIDTH_EDEFAULT;
			case GMFGraphPackage.POLYGON_DECORATION__LINE_KIND:
				return lineKind != LINE_KIND_EDEFAULT;
			case GMFGraphPackage.POLYGON_DECORATION__XOR_FILL:
				return xorFill != XOR_FILL_EDEFAULT;
			case GMFGraphPackage.POLYGON_DECORATION__XOR_OUTLINE:
				return xorOutline != XOR_OUTLINE_EDEFAULT;
			case GMFGraphPackage.POLYGON_DECORATION__RESOLVED_CHILDREN:
				return !getResolvedChildren().isEmpty();
			case GMFGraphPackage.POLYGON_DECORATION__TEMPLATE:
				return template != null && !template.isEmpty();
		}
		return eDynamicIsSet(eFeature);
	}

