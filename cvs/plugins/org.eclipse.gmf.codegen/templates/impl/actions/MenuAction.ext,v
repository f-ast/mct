import "http://www.eclipse.org/gmf/2008/GenModel";
import "http://www.eclipse.org/emf/2002/Ecore";

boolean hasCommandsToContribute(gmfgen::GenEditorGenerator editorGen) :
	!editorGen.contextMenus.isEmpty() // FIXME temp condition, need deep check 
;

boolean hasHandlersToContribute(gmfgen::GenEditorGenerator editorGen) :
	hasCommandsToContribute(editorGen) // exists(GenCustomAction)
;@
